<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
      <author>Sebastian Spier (http://twitter.com/#!/sebastianspier/)</author>
      <description>Search for institution</description>
      <documentationURL>http://anabin.kmk.org/no_cache/filter/institutionen.html</documentationURL>
      <sampleQuery>SELECT * FROM {table} WHERE </sampleQuery>
    </meta>
    <bindings>
        <select itemPath="*" produces="JSON">
            <urls>
                <url>http://anabin.kmk.org/index.php</url>
            </urls>
            <inputs>
              <key id="eID" type="xs:string" paramType="query" required="true" default="user_anabin_institutionen"/>
              <key id="conf" type="xs:string" paramType="query" required="true" default="institutionsergebnisliste"/>
              <key id="sSearch" type="xs:string" paramType="query" />
            </inputs>
        </select>
     </bindings>
</table>