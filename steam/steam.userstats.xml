<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
	<meta>
		<author>Andrew Guyton</author>
		<documentationURL>http://developer.valvesoftware.com/wiki/Steam_Web_API</documentationURL>
		<sampleQuery>select * from {table} where gameid='440'</sampleQuery>
	</meta>
	<bindings>
		<select itemPath="" produces="JSON">
			<urls>
				<url>http://api.steampowered.com/ISteamUserStats/GetGlobalAchievementPercentagesForApp/v0001/</url>
			</urls>
			<inputs>
				<key id="gameid" type="xs:string" paramType="query" required="true" />
				<key id="format" type="xs:string" paramType="query" required="false" />
			</inputs>
		</select>
	</bindings>
</table>
