<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
			<author>Sebastian Spier (http://twitter.com/#!/sebastianspier/)</author>
			<description></description>
			<documentationURL></documentationURL>
	    <sampleQuery>SELECT * FROM {table} WHERE apikey="YOUR_API_KEY"</sampleQuery>
    </meta>
    <bindings>
        <select itemPath="response.data" produces="JSON">
            <urls>
                <url>http://emailapi.dynect.net/rest/json/reports/bounces/</url>
            </urls>
						<paging model="offset">
							<start id="startindex" default="0" />
							<pagesize id="per_page" max="500" />
							<total default="500" />
						</paging>
            <inputs>
                <key id="apikey" type="xs:string" paramType="query" required="true" />
								<key id="startdate" type="xs:string" paramType="query" />
								<key id="enddate" type="xs:string" paramType="query" />
            </inputs>
        </select>
     </bindings>
</table>



